<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fr.doranco.eboutique.entity.Adresse" table="adresse">
		<id name="id" type="java.lang.Integer">
			<column name="id" sql-type="int(11)"></column>
			<generator class="increment"></generator>
		</id>
		<property name="numero" type="java.lang.Integer">
			<column name="numero" sql-type="int(4)"></column>
		</property>
		<property name="rue" type="java.lang.String">
			<column name="rue" sql-type="varchar(45)"></column>
		</property>
		<property name="ville" type="java.lang.String">
			<column name="ville" sql-type="varchar(20)"></column>
		</property>
		<property name="codePostal" type="java.lang.String">
			<column name="code_postal" sql-type="varchar(5)"></column>
		</property>

	</class>

</hibernate-mapping>