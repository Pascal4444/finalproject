<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fr.doranco.eboutique.entity.Params" table="params">
		<id name="id" type="java.lang.Integer">
			<column name="id" sql-type="int(11)"></column>
			<generator class="increment"></generator>
		</id>
		<property name="numero" type="java.lang.Integer">
			<column name="cle_cryptage_pwd" sql-type="BLOB"></column>
		</property>
		<property name="rue" type="java.lang.String">
			<column name="cle_cryptage_cp" sql-type="BLOB"></column>
		</property>

	</class>

</hibernate-mapping>