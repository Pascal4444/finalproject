<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fr.doranco.eboutique.entity.LigneDeCommande" table="ligne_de_commande">
		<id name="id" type="java.lang.Integer">
			<column name="id" sql-type="int(11)"></column>
			<generator class="increment"></generator>
		</id>
		<property name="numero" type="java.lang.Integer">
			<column name="quantite" sql-type="int(2)"></column>
		</property>
		<property name="rue" type="java.lang.String">
			<column name="prix_unitaire" sql-type="DOUBLE"></column>
		</property>
		<property name="rue" type="java.lang.String">
			<column name="remise_article" sql-type="DOUBLE"></column>
		</property>
		<property name="ville" type="java.lang.String">
			<column name="commande" sql-type="varchar(45)"></column>
		</property>
		<property name="codePostal" type="java.lang.String">
			<column name="article" sql-type="varchar(45)"></column>
		</property>

	</class>

</hibernate-mapping>