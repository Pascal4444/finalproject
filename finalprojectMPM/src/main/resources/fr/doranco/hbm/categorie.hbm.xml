<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fr.doranco.eboutique.entity.Categorie" table="categorie">
		<id name="id" type="java.lang.Integer">
			<column name="id" sql-type="int(11)"></column>
			<generator class="increment"></generator>
		</id>
		<property name="rue" type="java.lang.String">
			<column name="nom" sql-type="varchar(45)"></column>
		</property>
		<property name="numero" type="java.lang.Integer">
			<column name="remise" sql-type="int(3)"></column>
		</property>
		<property name="ville" type="java.lang.String">
			<column name="is_remise_cumulable" sql-type="ENUM('true','false')"></column>
		</property>
		<property name="ville" type="java.lang.String">
			<column name="photo" sql-type="varchar(45)"></column>
		</property>
		<property name="codePostal" type="java.lang.String">
			<column name="articles" sql-type="varchar(45)"></column>
		</property>

	</class>

</hibernate-mapping>